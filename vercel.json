{
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/ws",
            "dest": "app.py",
            "methods": ["GET", "POST"]
        },
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ],
    "env": {
        "PYTHONPATH": "${{secrets.PYTHONPATH}}"
    }
}